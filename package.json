{
  "name": "urdfjsx",
  "description": "A tool to convert URDF into react three fiber JSX component",
  "version": "0.1.2",
  "license": "MIT",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "bin": "bin/urdfjsx.js",
  "repository": "wty-andrew/urdfjsx",
  "keywords": [
    "urdf",
    "react-three-fiber"
  ],
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "tsx watch src/cli/index.ts",
    "build": "tsc --project tsconfig.build.json",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest",
    "lint": "biome check src",
    "prebuild": "rimraf dist"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.10",
    "@types/ramda": "^0.29.3",
    "@types/yargs": "^17.0.24",
    "jest": "^29.6.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.2"
  },
  "packageManager": "yarn@3.6.1",
  "dependencies": {
    "@babel/generator": "^7.22.10",
    "@babel/parser": "^7.22.10",
    "@babel/traverse": "^7.22.10",
    "@babel/types": "^7.22.10",
    "fast-xml-parser": "^4.2.7",
    "prettier": "^3.0.1",
    "ramda": "^0.29.0",
    "typescript": "^5.1.6",
    "yargs": "^17.7.2"
  }
}
