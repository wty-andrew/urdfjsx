{
  "name": "urdfjsx",
  "description": "A tool to convert URDF into react three fiber JSX component",
  "version": "0.1.2",
  "license": "MIT",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "bin": "bin/urdfjsx.js",
  "repository": "wty-andrew/urdfjsx",
  "keywords": [
    "urdf",
    "react-three-fiber"
  ],
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "tsx watch src/cli/index.ts",
    "build": "tsc --project tsconfig.build.json",
    "test": "vitest",
    "lint": "biome check src",
    "prebuild": "rimraf dist"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.2",
    "@types/ramda": "^0.30.2",
    "@types/yargs": "^17.0.33",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@babel/generator": "8.0.0-alpha.14",
    "@babel/parser": "8.0.0-alpha.14",
    "@babel/traverse": "8.0.0-alpha.14",
    "@babel/types": "8.0.0-alpha.14",
    "fast-xml-parser": "^4.5.0",
    "prettier": "^3.4.2",
    "ramda": "^0.30.1",
    "typescript": "^5.7.2",
    "yargs": "^17.7.2"
  }
}
